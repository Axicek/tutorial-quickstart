parameters:


php:
	date.timezone: Europe/Prague


nette:
	application:
		errorPresenter: Error
		mapping:
			*: App\*Module\Presenters\*Presenter

	session:
		expiration: 14 days

	security:
		users:
			admin: secret  # user 'admin', password 'secret'

services:
	router: App\RouterFactory::createRouter
